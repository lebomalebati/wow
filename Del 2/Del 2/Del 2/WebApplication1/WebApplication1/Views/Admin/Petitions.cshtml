
@{
    ViewBag.Title = "Petitions";
    Layout = "~/Views/Shared/_Layout - Copy.cshtml";
}<h1>The Petitions</h1>
<br />
<a class="btn btn-primary " href="@Url.Action("PetitionsCreate","Admin")">New Petition</a>
<br />
<br />
<br />

<form>
    <div class="piece">
        <h2 class="sectionHeader">Petition 1</h2>
        <div class="two">
            <div class="car" id="veloster">

                <table style="width:100%">
                    <tr>
                        <td>RAINN is once again partnering with Google to ensure that survivors of sexual violence get the support they need and deserve through the National Sexual Assault Hotline and rainn.org.</td>
                        <td><img src="./images/hyundaiVeloster.jpg" width="640" height="320" alt="Partnership 1"></td>
                    </tr>

                    <tr>
                        <td colspan="2" style="text-align:center">
                            <br /><a class="btn btn-link disabled" href="@Url.Action("More", "Program")">Read more...</a>
                        </td>
                    </tr>

                </table>

            </div>
        </div>
    </div>
    <a class="btn btn-danger " href="@Url.Action("PetitionsUpdate","Admin")">Update</a>
    <a class="btn btn-danger " href="@Url.Action("PetitionsDelete","Admin")">Delete</a>

</form>

<br />
<br />

